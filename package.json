{
  "name": "cargo-actions",
  "version": "1.0.0",
  "private": true,
  "description": "Integration Rust Cargo with GitHub Actions",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fibiol/cargo-actions.git"
  },
  "keywords": [
    "actions",
    "github",
    "rust",
    "cargo"
  ],
  "author": "Andrey Savitsky <contact@qroc.pro>",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^5.0.3"
  },
  "devDependencies": {
    "@types/node": "^18.7.16",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.23.0",
    "eslint-plugin-github": "^4.3.7",
    "eslint-plugin-jest": "^27.0.2",
    "jest": "^27.2.5",
    "jest-circus": "^26.6.3",
    "js-yaml": "^4.1.0",
    "nock": "^13.0.11",
    "prettier": "^2.7.1",
    "svg-element-attributes": "^1.3.1",
    "ts-jest": "^27.1.2",
    "typescript": "^4.2.3"
  }
}