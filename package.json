{
  "name": "cargo-actions",
  "version": "1.0.0",
  "private": true,
  "description": "Integration Rust Cargo with GitHub Actions",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fibiol/cargo-actions.git"
  },
  "keywords": [
    "actions",
    "github",
    "rust",
    "cargo"
  ],
  "author": "Andrey Savitsky <contact@qroc.pro>",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^5.1.1",
    "@typescript-eslint/parser": "^5.41.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.4",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.26.0",
    "eslint-plugin-github": "^4.4.0",
    "eslint-plugin-jest": "^27.1.3",
    "jest": "^29.2.1",
    "jest-circus": "^29.2.1",
    "js-yaml": "^4.1.0",
    "nock": "^13.2.9",
    "prettier": "^2.7.1",
    "svg-element-attributes": "^2.0.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  }
}
